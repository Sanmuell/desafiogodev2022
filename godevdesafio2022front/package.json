{
    "name": "godev-2022-desafio-frontend",
    "version": "1.0.0",
    "engines": { "node": ">=10.14.0", "npm": ">=6.4.1" },
    "scripts": {
        "start": "node server.js",
        "translations": "node translations.js",
        "ng-high-memory": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
        "build": "npm run translations && ng build --prod",
        "build:high-memory": "npm run translations && npm run ng-high-memory build --prod",
        "build:stats": "npm run build -- --stats-json && npx webpack-bundle-analyzer dist/stats.json",
        "test": "ng test --watch=false",
        "test:dev": "ng test",
        "test:coverage": "ng test --watch=false --code-coverage && npx http-server coverage -o",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "project": {
        "app": "godev2022desafio",
        "domain": "godev",
        "service": "godev2022desafio",
        "serviceDependencies": [{ "service": "godev2022desafio", "version": "1.0.0" }]
    },
    "keywords": ["ES8", "JavaScript", "TypeScript", "Angular", "Senior"],
